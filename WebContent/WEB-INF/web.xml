<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>BattleshipWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
    <servlet>
    <servlet-name>StartBattleshipServlet</servlet-name>
    <servlet-class>bs.controller.StartBattleshipServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StartBattleshipServlet</servlet-name>
    <url-pattern>/startBattle</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ShipSetServlet</servlet-name>
    <servlet-class>bs.controller.ShipSetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShipSetServlet</servlet-name>
    <url-pattern>/shipSetting</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>FireMissileServlet</servlet-name>
    <servlet-class>bs.controller.FireMissileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FireMissileServlet</servlet-name>
    <url-pattern>/fireMissile</url-pattern>
  </servlet-mapping>
  
</web-app>